{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/prefer-default-export": "off",
    "no-unused-vars": "warn",
    "no-trailing-spaces": "off",
    "import/extensions": "off",
    "quotes": "off",
    "arrow-body-style": "warn",
    "camelcase": "off"
  }
}
